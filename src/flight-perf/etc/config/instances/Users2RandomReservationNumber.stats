    flight {
        stats {
            instance {
                id = "Users2RandomReservationNumber"

                runId = "Users2RandomReservationNumber"

                filterId = ""

                numberSamples = 3 // was 10
                numberUsersPerSample = 2
            }
        }
    }
}
